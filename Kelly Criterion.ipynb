   "source": [
    "kc_df = None \n",
    "\n",
    "def compute():\n",
    "    global kc_df\n",
    "\n",
    "    # Get the values from the input fields\n",
    "    ticker = entries[0].get()\n",
    "    p = float(entries[1].get())\n",
    "    cp = float(entries[2].get())\n",
    "    profit_price = float(entries[3].get())\n",
    "    loss_price = float(entries[4].get())\n",
    "    xk = float(entries[5].get())\n",
    "    aum = float(entries[6].get())\n",
    "    r = float(entries[7].get())\n",
    "    sr = float(entries[8].get())\n",
    "    leverage = float(entries[9].get())\n",
    "    datetime = pd.Timestamp.today().floor('D')\n",
    "    date_string = str(datetime.date())\n",
    "    \n",
    "    # Perform the computation\n",
    "     # Perform the computation\n",
    "    size = trade_sizing(p=p, cp=cp, profit_price=profit_price, loss_price=loss_price, xk=xk, aum=aum, r=r, sr=sr, leverage=leverage)\n",
    "\n",
    "    # Display the result\n",
    "    result_label.config(text=f\"Result: {size}\")\n",
    "\n",
    "    # Create a DataFrame with the input values and result\n",
    "    data = {\n",
    "        'Date': [pd.to_datetime('today').normalize()],\n",
    "        \"Ticker\": [ticker],\n",
    "        'Current Price': [cp],\n",
    "        \"Probability of Win\": [p],\n",
    "        \"Leverage\": [leverage],\n",
    "        \"Potential Profit (%)\": [(abs(profit_price - cp) / cp) * 100 * leverage],\n",
    "        \"Potential Loss (%)\": [(abs(loss_price - cp) / cp) *100 * leverage],\n",
    "        \"Kelly Sizing\": [xk],\n",
    "        \"Total Risk Available\": [aum * r],\n",
    "        \"Risk Available for this trade\": [sr],\n",
    "        'Trade Size (Number of contracts)': [size],\n",
    "        'Position Size': [size * cp],\n",
    "        'Risk allocated': [size * cp * ((abs(loss_price - cp) / cp) * leverage)],\n",
    "        'Portfolio Size (% of AUM)': [((cp*size)/aum)*100]\n",
    "    }\n",
    "    \n",
    "    kc_df = pd.DataFrame(data).round(2)\n",
    "\n",
    "    # Check if the Kelly Criterion file exists\n",
    "\n",
    "    if os.path.exists('Kelly Criterion.xlsx'):   \n",
    "        # Load the existing Kelly Criterion file\n",
    "        kc_df_old = pd.read_excel('Kelly Criterion.xlsx')\n",
    "        # Append kc_df to the existing file\n",
    "        kc_df_old = pd.concat([kc_df_old, kc_df], ignore_index=True)\n",
    "        kc_df = kc_df_old\n",
    "\n",
    "    else:\n",
    "        # Create the Kelly Criterion file\n",
    "        kc_df = kc_df\n",
    "\n",
    "    # Save kc_df to Excel file\n",
    "    kc_df.to_excel('Kelly Criterion.xlsx', index=False)\n",
    "\n",
    "# Create the main window\n",
    "window = tk.Tk()\n",
    "window.title(\"Kelly Criterion\")\n",
    "\n",
    "# Create the input fields\n",
    "labels = [\"Ticker\", \"Probability of Win:\", \"Current Price:\", \"Profit Price:\",\"Loss Price:\", \"Kelly Sizing\", 'AUM', \"Total Risk Available\", \"Risk Available for this trade\", \"Leverage:\"]\n",
    "entries = []\n",
    "for i in range(10):\n",
    "    label = tk.Label(window, text=labels[i])\n",
    "    label.grid(row=i, column=0, padx=10, pady=5)\n",
    "    \n",
    "    entry = tk.Entry(window)\n",
    "    entry.grid(row=i, column=1, padx=10, pady=5)\n",
    "    \n",
    "    entries.append(entry)\n",
    "\n",
    "# Create the compute button\n",
    "compute_button = tk.Button(window, text=\"Compute\", command=compute)\n",
    "compute_button.grid(row=14, column=0, columnspan=2, padx=10, pady=10)\n",
    "\n",
    "# Create the label for displaying the result\n",
    "result_label = tk.Label(window, text=\"Result: \")\n",
    "result_label.grid(row=15, column=0, columnspan=2, padx=10, pady=5)\n",
    "\n",
    "# Start the main event loop\n",
    "window.mainloop()\n"
   ]
